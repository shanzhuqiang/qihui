<!--pages/qucanName/qucanName.wxml-->
<view class="qucanNameWrap">
  <view class="iptRow">
    <view class="label">取餐人姓名</view>
    <view class="iptBox">
      <input class="ipt" placeholder="请输入姓名"></input>
    </view>
  </view>
  <view class="iptRow">
    <view class="label">手机号码</view>
    <view class="iptBox">
      <input class="ipt" placeholder="请输入手机号码" type="number" maxlength="11"></input>
    </view>
  </view>
  <view class="saveBtn">保存</view>
</view>
